{
  "project": {
    "version": "v1.0.0",
    "name": "htmlclient"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist/web",
      "assets": [
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "hc",
      "mobile": false,
      "styles": [
        "../node_modules/font-awesome/css/font-awesome.css",
        "../node_modules/jqwidgets-framework/jqwidgets/styles/jqx.base.css",
        "styles.scss"
      ],
      "scripts": [
        "../node_modules/hammerjs/hammer.js",
        "../node_modules/jquery/dist/jquery.js",
        "../node_modules/jqwidgets-framework/jqwidgets/jqxcore.js",
        "../node_modules/jqwidgets-framework/jqwidgets/jqxtooltip.js",
        "../node_modules/bootstrap-sass/assets/javascripts/bootstrap.js",
        "scripts.js"
      ],
      "environmentSource": "env/env.dev.ts",
      "environments": {
        "dev": "env/env.dev.ts",
        "prod": "env/env.prod.ts"
      }
    }
  ],
  "addons": [
    "../node_modules/font-awesome/fonts/*.+(otf|eot|svg|ttf|woff|woff2)"
  ],
  "defaults": {
    "styleExt": "scss",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": false,
      "directive": false,
      "module": false,
      "pipe": false,
      "service": false
    }
  }
}
