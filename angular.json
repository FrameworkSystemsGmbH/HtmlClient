{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "hc": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "hc",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "allowedCommonJsDependencies": [
              "moment-timezone",
              "overlayscrollbars"
            ],
            "outputPath": {
              "base": "dist/build/dev"
            },
            "index": "src/index.html",
            "polyfills": [
              "src/init",
              "zone.js",
              "src/ext/array/array",
              "src/ext/boolean/boolean",
              "src/ext/error/error",
              "src/ext/math/math",
              "src/ext/number/number",
              "src/ext/string/string"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [
              "node_modules/hammerjs/hammer.min.js"
            ],
            "browser": "src/main.ts",
            "stylePreprocessorOptions": {
              "includePaths": [
                "."
              ]
            }
          },
          "configurations": {
            "prod": {
              "outputPath": {
                "base": "dist/build/prod",
                "browser": ""
              },
              "fileReplacements": [
                {
                  "replace": "src/env/env.dev.ts",
                  "with": "src/env/env.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "dev": {
              "outputPath": {
                "base": "dist/build/dev",
                "browser": ""
              },
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "prod"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "prod": {
              "buildTarget": "hc:build:prod"
            },
            "dev": {
              "buildTarget": "hc:build:dev"
            }
          },
          "defaultConfiguration": "dev"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n",
          "options": {
            "buildTarget": "hc:build"
          }
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [
              "src/init",
              "zone.js",
              "zone.js/testing",
              "src/ext/array/array",
              "src/ext/boolean/boolean",
              "src/ext/error/error",
              "src/ext/math/math",
              "src/ext/number/number",
              "src/ext/string/string"
            ],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [
              "node_modules/hammerjs/hammer.min.js"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.js",
              "src/**/*.ts",
              "src/**/*.d.ts",
              "src/**/*.html"
            ],
            "eslintConfig": "eslint.config.js"
          }
        }
      }
    }
  },
  "cli": {
    "schematicCollections": [
      "@angular-eslint/schematics"
    ],
    "analytics": false
  },
  "schematics": {
    "@schematics/angular:component": {
      "type": "component"
    },
    "@schematics/angular:directive": {
      "type": "directive"
    },
    "@schematics/angular:service": {
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  }
}
