{
  "name": "htmlclient",
  "version": "1.0.30",
  "description": "Framework Studio HTML Client",
  "repository": "npm/npm",
  "private": true,
  "license": "none",
  "readme": "readme.md",
  "scripts": {
    "ngcc": "ngcc",
    "build": "ng build",
    "buildProd": "npm run buildProdClean && ng build --prod && npm run buildProdPack",
    "buildProdClean": "del-cli \"dist/webzip\"",
    "buildProdPack": "mkdirp \"dist/webzip\" && cd \"dist/web\" && bestzip \"../webzip/HtmlClient.zip\" *",
    "lint": "ng lint",
    "lint-imports": "tslint --config tslint-imports.json --fix --project .",
    "test": "ng test",
    "serve": "ng serve",
    "serveProd": "ng serve --prod",
    "servePublic": "ng serve --host=0.0.0.0 --disable-host-check",
    "serveProdPublic": "ng serve --prod --host=0.0.0.0 --disable-host-check",
    "cdBuild": "npm run buildProd && npm run cdClean && npm run cdCopy && npm run cdInject && npm run cdPlatform && npm run cdPrepare && npm run cdCompile",
    "cdClean": "del-cli \"dist/cordova\"",
    "cdCopy": "mkdirp \"dist/cordova/www\" && ncp \"dist/web\" \"dist/cordova/www\" && cpy \"platforms/cordova/**/*.*\" \"!platforms/cordova/inject.*\" \"dist/cordova\"",
    "cdInject": "replace-in-file --configFile=\"platforms/cordova/inject.cordova.json\"",
    "cdPlatform": "cd \"dist/cordova\" && cordova platform add android",
    "cdPrepare": "cd \"dist/cordova\" && cordova prepare android",
    "cdCompile": "cd \"dist/cordova\" && cordova build android",
    "pgBuild": "npm run build && npm run pgClean && npm run pgCopy && npm run pgInject && npm run pgPack && npm run pgPurge",
    "pgBuildProd": "npm run buildProd && npm run pgClean && npm run pgCopy && npm run pgInject && npm run pgPack && npm run pgPurge",
    "pgClean": "del-cli \"dist/phonegap\"",
    "pgCopy": "mkdirp \"dist/phonegap/web\" && ncp \"dist/web\" \"dist/phonegap/web\" && cpy \"platforms/cordova/**/*.*\" \"!platforms/cordova/inject.*\" \"dist/phonegap/web\"",
    "pgInject": "replace-in-file --configFile=\"platforms/cordova/inject.phonegap.json\"",
    "pgPack": "cd \"dist/phonegap/web\" && bestzip \"../app.zip\" *",
    "pgPurge": "del-cli \"dist/phonegap/web\"",
    "elBuild": "npm run buildProd && npm run elClean && npm run elCopy && npm run elComment && npm run elPack && npm run elPurge",
    "elClean": "del-cli \"dist/electron/**/*\" \"!dist/electron/cache\" \"!dist/electron/cache/**/*\"",
    "elCopy": "mkdirp \"dist/electron/web\" && ncp \"dist/web\" \"dist/electron/web\" && ncp \"package.json\" \"dist/electron/web/package.json\" && ncp \"platforms/electron/index.js\" \"dist/electron/web/index.js\" && ncp \"platforms/electron/icons/icon.png\" \"dist/electron/web/icon.png\"",
    "elComment": "remove-html-comments \"dist/electron/web/index.html\" \"dist/electron/web/index.html\"",
    "elPack": "electron-packager \"./dist/electron/web\" --asar --download.cache=\"./dist/electron/cache\" --electron-version=\"9.0.5\" --out=\"./dist/electron\" --platform=\"win32,linux,darwin\" --arch=x64 --overwrite --no-tempdir",
    "elPurge": "del-cli \"dist/electron/web\""
  },
  "dependencies": {
    "@angular/animations": "8.2.14",
    "@angular/cdk": "8.2.3",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/forms": "8.2.14",
    "@angular/material": "8.2.3",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/platform-webworker": "8.2.14",
    "@angular/router": "8.2.14",
    "@fortawesome/fontawesome": "1.1.8",
    "@fortawesome/fontawesome-free-solid": "5.0.13",
    "@ngrx/store": "8.6.0",
    "hammerjs": "2.0.8",
    "intl": "1.2.5",
    "moment": "2.27.0",
    "moment-timezone": "0.5.31",
    "normalize.css": "8.0.1",
    "overlayscrollbars": "1.12.0",
    "overlayscrollbars-ngx": "0.2.2",
    "rxjs": "6.5.5",
    "tslib": "1.10.0",
    "web-animations-js": "2.3.2",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.28",
    "@angular/cli": "8.3.28",
    "@angular/compiler-cli": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@types/cordova": "0.0.34",
    "@types/jasmine": "3.5.11",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "^12.12.47",
    "@types/overlayscrollbars": "1.12.0",
    "bestzip": "2.1.5",
    "codelyzer": "5.2.2",
    "cordova": "9.0.0",
    "cordova-plugin-camera": "4.1.0",
    "cordova-plugin-device": "2.0.3",
    "cordova-plugin-file": "6.0.2",
    "cordova-plugin-geolocation": "4.0.2",
    "cordova-plugin-inappbrowser": "4.0.0",
    "cordova-plugin-ionic-keyboard": "2.2.0",
    "cordova-plugin-ionic-webview": "5.0.0",
    "cordova-plugin-navigationbar": "1.0.31",
    "cordova-plugin-splashscreen": "5.0.4",
    "cordova-plugin-statusbar": "2.4.3",
    "cordova-plugin-whitelist": "1.3.4",
    "cpy-cli": "3.1.1",
    "del-cli": "3.0.1",
    "electron": "9.0.5",
    "electron-packager": "15.0.0",
    "jasmine": "3.5.0",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "5.1.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "3.3.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "mkdirp": "1.0.4",
    "ncp": "2.0.0",
    "node-sass": "4.14.1",
    "phonegap-plugin-barcodescanner": "8.1.0",
    "remove-html-comments": "1.0.2",
    "replace-in-file": "6.1.0",
    "rxjs-tslint": "0.1.8",
    "ts-node": "8.10.2",
    "tslint": "5.20.1",
    "tslint-etc": "1.11.1",
    "tsutils": "3.17.1",
    "typescript": "3.5.3"
  }
}
