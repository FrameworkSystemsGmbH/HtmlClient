    "moment": "2.29.1",
    "moment-timezone": "0.5.33",
    "normalize.css": "8.0.1",
    "overlayscrollbars": "1.13.1",
    "overlayscrollbars-ngx": "0.2.3",
    "rxjs": "6.6.7",
    "tslib": "2.1.0",
