    "ng": "ng",
    "lint": "ng lint",
    "test": "ng test",
    "buildDev": "ng build -c dev",
    "buildProd": "ng build -c prod",
    "serveDev": "ng serve -c dev",
    "serveProd": "ng serve -c prod",
    "serveDevPublic": "ng serve -c dev --host \"0.0.0.0\"",
    "serveProdPublic": "ng serve -c prod --host=\"0.0.0.0\"",
    "publish": "npm run lint && npm run buildProd && del-cli \"dist/release\" && mkdirp \"dist/release\" && powershell -Command \"Set-Location 'dist/build/prod'; Compress-Archive -Path * -DestinationPath '../../release/HtmlClient.zip' -Force\"",
    "capMigrate": "cap migrate",
    "capSync": "cap sync android",
    "updateSim": "ncu",
    "updateRun": "ncu -u"
