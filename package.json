{
  "name": "htmlclient",
  "version": "1.0.5",
  "description": "Framework Studio HTML Client",
  "repository": "npm/npm",
  "private": true,
  "license": "none",
  "readme": "readme.md",
  "scripts": {
    "build": "ng build",
    "buildProd": "npm run buildProdClean && ng build --prod && npm run buildProdPack",
    "buildProdClean": "del-cli \"dist/webzip\"",
    "buildProdPack": "mkdirp \"dist/webzip\" && bestzip \"dist/webzip/HtmlClient.zip\" \"dist/web/*\"",
    "lint": "ng lint",
    "test": "ng test",
    "serve": "ng serve",
    "servePublic": "ng serve --host=0.0.0.0 --disable-host-check",
    "cdBuild": "npm run buildProd && npm run cdClean && npm run cdCopy && npm run cdInject && npm run cdPlatform && npm run cdPrepare && npm run cdCompile",
    "cdClean": "del-cli \"dist/cordova\"",
    "cdCopy": "mkdirp \"dist/cordova/www\" && ncp \"dist/web\" \"dist/cordova/www\" && cpy \"platforms/cordova/**/*.*\" \"!platforms/cordova/cordova.js\" \"dist/cordova\"",
    "cdInject": "ncp \"dist/cordova/www/index.html\" \"dist/cordova/www/index_inject.html\" && postbuild -i \"dist/cordova/www/index_inject.html\" -o \"dist/cordova/www/index.html\" -j \"platforms/cordova/cordova.js\" -g \"platforms/cordova/\" && del-cli \"dist/cordova/www/index_inject.html\"",
    "cdPlatform": "cd \"dist/cordova\" && cordova platform add android",
    "cdPrepare": "cd \"dist/cordova\" && cordova prepare android",
    "cdCompile": "cd \"dist/cordova\" && cordova build android",
    "pgBuild": "npm run buildProd && npm run pgClean && npm run pgCopy && npm run pgInject && npm run pgPack && npm run pgPurge",
    "pgClean": "del-cli \"dist/phonegap\"",
    "pgCopy": "mkdirp \"dist/phonegap/web\" && ncp \"dist/web\" \"dist/phonegap/web\" && cpy \"platforms/cordova/**/*.*\" \"!platforms/cordova/cordova.js\" \"dist/phonegap/web\"",
    "pgInject": "ncp \"dist/phonegap/web/index.html\" \"dist/phonegap/web/index_inject.html\" && postbuild -i \"dist/phonegap/web/index_inject.html\" -o \"dist/phonegap/web/index.html\" -j \"platforms/cordova/cordova.js\" -g \"platforms/cordova/\" && del-cli \"dist/phonegap/web/index_inject.html\"",
    "pgPack": "bestzip \"dist/phonegap/app.zip\" \"dist/phonegap/web/*\"",
    "pgPurge": "del-cli \"dist/phonegap/web\"",
    "elBuild": "npm run buildProd && npm run elClean && npm run elCopy && npm run elComment && npm run elPack && npm run elPurge",
    "elClean": "del-cli \"dist/electron/**/*\" \"!dist/electron/cache\" \"!dist/electron/cache/**/*\"",
    "elCopy": "mkdirp \"dist/electron/web\" && ncp \"dist/web\" \"dist/electron/web\" && ncp \"package.json\" \"dist/electron/web/package.json\" && ncp \"platforms/electron/index.js\" \"dist/electron/web/index.js\" && ncp \"platforms/electron/icons/icon.png\" \"dist/electron/web/icon.png\"",
    "elComment": "remove-html-comments \"dist/electron/web/index.html\" \"dist/electron/web/index.html\"",
    "elPack": "electron-packager \"./dist/electron/web\" --asar --download.cache=\"./dist/electron/cache\" --electron-version=\"2.0.2\" --out=\"./dist/electron\" --platform=\"win32,linux,darwin\" --arch=x64 --overwrite --no-tempdir",
    "elPurge": "del-cli \"dist/electron/web\""
  },
  "dependencies": {
    "@angular/animations": "6.0.5",
    "@angular/cdk": "6.2.1",
    "@angular/common": "6.0.5",
    "@angular/compiler": "6.0.5",
    "@angular/core": "6.0.5",
    "@angular/forms": "6.0.5",
    "@angular/http": "6.0.5",
    "@angular/material": "6.2.1",
    "@angular/platform-browser": "6.0.5",
    "@angular/platform-browser-dynamic": "6.0.5",
    "@angular/router": "6.0.5",
    "@fortawesome/fontawesome": "1.1.8",
    "@fortawesome/fontawesome-free-solid": "5.0.13",
    "@ngrx/store": "6.0.1",
    "core-js": "2.5.7",
    "hammerjs": "2.0.8",
    "intl": "1.2.5",
    "moment": "2.22.2",
    "moment-timezone": "0.5.17",
    "normalize.css": "8.0.0",
    "rxjs": "6.2.1",
    "web-animations-js": "2.3.1",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.6.8",
    "@angular/cli": "6.0.8",
    "@angular/compiler-cli": "6.0.5",
    "@angular/language-service": "6.0.5",
    "@types/cordova": "0.0.34",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "^8.0.0",
    "bestzip": "1.1.4",
    "codelyzer": "4.3.0",
    "cordova": "8.0.0",
    "cordova-plugin-device": "2.0.2",
    "cordova-plugin-file": "6.0.1",
    "cordova-plugin-ionic-keyboard": "2.1.2",
    "cordova-plugin-ionic-webview": "1.2.1",
    "cordova-plugin-splashscreen": "5.0.2",
    "cordova-plugin-whitelist": "1.3.3",
    "cpy-cli": "2.0.0",
    "del-cli": "1.1.0",
    "electron-packager": "12.1.0",
    "jasmine": "3.1.0",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.2",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.1.0",
    "mkdirp": "0.5.1",
    "ncp": "2.0.0",
    "node-sass": "4.9.0",
    "postbuild": "2.1.0",
    "remove-html-comments": "1.0.2",
    "rxjs-tslint": "0.1.4",
    "ts-node": "6.1.1",
    "tslint": "5.10.0",
    "typescript": "2.7.2"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "IE 11"
  ]
}
