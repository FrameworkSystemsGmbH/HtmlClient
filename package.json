{
  "name": "htmlclient",
  "version": "1.0.40",
  "description": "Framework Studio HTML Client",
  "repository": "npm/npm",
  "private": true,
  "license": "none",
  "readme": "README.md",
  "scripts": {
    "build": "ng build -c dev",
    "buildProd": "npm run buildProdClean && ng build -c prod && npm run buildProdPack",
    "buildProdClean": "del-cli \"dist/release\"",
    "buildProdPack": "mkdirp \"dist/release\" && cd \"dist/build/prod\" && bestzip \"../../release/HtmlClient.zip\" *",
    "lint": "ng lint",
    "test": "ng test",
    "serve": "ng serve -c dev",
    "serveProd": "ng serve -c prod",
    "servePublic": "ng serve -c dev --host=0.0.0.0 --disable-host-check",
    "serveProdPublic": "ng serve -c prod --host=0.0.0.0 --disable-host-check",
    "sync": "cap sync android"
  },
