{
  "name": "htmlclient",
  "version": "v1.0.0",
  "description": "Framework Studio HTML Client",
  "repository": "npm/npm",
  "private": true,
  "license": "none",
  "readme": "readme.md",
  "scripts": {
    "build": "ng build --prod --no-sourcemap --vendor-chunk false",
    "lint": "tslint \"src/**/*.ts\" --project \"src/tsconfig.json\" --type-check",
    "serve": "ng serve",
    "buildElectron": "npm run build && gulp electron",
    "initCordova": "npm run build && gulp cordova.initialize",
    "buildCordovaAndroid": "npm run build && gulp cordova.build.android",
    "emulateCordovaAndroid": "gulp cordova.emulate.android",
    "buildCordovaWindows": "npm run build && gulp cordova.build.windows",
    "emulateCordovaWindows": "gulp cordova.emulate.windows"
  },
  "dependencies": {
    "@angular/animations": "4.3.5",
    "@angular/common": "4.3.5",
    "@angular/compiler": "4.3.5",
    "@angular/core": "4.3.5",
    "@angular/forms": "4.3.5",
    "@angular/http": "4.3.5",
    "@angular/platform-browser": "4.3.5",
    "@angular/platform-browser-dynamic": "4.3.5",
    "@angular/router": "4.3.5",
    "bootstrap-sass": "3.3.7",
    "core-js": "2.5.0",
    "font-awesome": "4.7.0",
    "hammerjs": "2.0.8",
    "jquery": "3.2.1",
    "moment": "2.18.1",
    "rxjs": "5.4.3",
    "zone.js": "0.8.16"
  },
  "devDependencies": {
    "@angular/cli": "1.3.1",
    "@angular/compiler-cli": "4.3.5",
    "@types/jasmine": "2.5.54",
    "@types/node": "8.0.24",
    "codelyzer": "3.1.2",
    "cordova": "7.0.1",
    "del": "3.0.0",
    "gulp": "3.9.1",
    "gulp-electron": "0.1.3",
    "node-sass": "4.5.3",
    "run-sequence": "2.1.0",
    "shelljs": "0.7.8",
    "ts-node": "3.3.0",
    "tslint": "5.6.0",
    "typescript": "2.4.2"
  }
}
